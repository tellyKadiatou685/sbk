{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "api/cron.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
 "crons": [
  {
    "path": "/api/cron",
    "schedule": "29 12 * * *"
  }
]
}